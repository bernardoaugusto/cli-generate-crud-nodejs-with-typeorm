{
    "name": "node-crud-typeorm",
    "version": "0.0.19",
    "description": "node-crud-typeorm CLI",
    "bin": {
        "node-crud-typeorm": "bin/node-crud-typeorm"
    },
    "scripts": {
        "clean-build": "rm -rf ./dist",
        "compile": "tsc -p .",
        "copy-templates": "if [ -e ./src/templates ]; then cp -a ./src/templates ./build/; fi",
        "build": "yarn clean-build && yarn compile && yarn copy-templates-seidor",
        "prepublishOnly": "yarn build",
        "test": "jest",
        "watch": "jest --watch",
        "snapupdate": "jest --updateSnapshot",
        "coverage": "jest --coverage",
        "copy-templates-seidor": "cp -r ./src/templates ./dist/templates",
        "build-seidor": "tsc && npm run copy-templates"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bernardoaugusto/cli-generate-crud-nodejs-with-typeorm.git"
    },
    "license": "MIT",
    "dependencies": {
        "gluegun": "latest",
        "lodash": "^4.17.20"
    },
    "devDependencies": {
        "@seidor-cloud-produtos/lib-seidor-common": "0.0.6",
        "@seidor-cloud-produtos/typeorm": "0.0.4",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.15",
        "@types/lodash": "^4.14.168",
        "@types/node": "^12.7.11",
        "@types/yup": "^0.29.8",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "@types/sinon": "^9.0.8",
        "@types/supertest": "^2.0.10",
        "@types/cors": "^2.8.8",
        "@types/pino-http": "^5.0.5",
        "@types/uuid": "^8.3.0",
        "pino-http": "^5.3.0",
        "express-async-errors": "^3.1.1",
        "cors": "^2.8.5",
        "supertest": "^5.0.0",
        "sinon": "^9.2.0",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.13.0",
        "eslint-import-resolver-typescript": "^2.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "express": "^4.17.1",
        "jest": "^26.6.1",
        "moment": "^2.29.1",
        "prettier": "^2.1.2",
        "reflect-metadata": "^0.1.13",
        "sqlite3": "5.0.0",
        "ts-jest": "^26.1.2",
        "ts-node": "^9.0.0",
        "tsyringe": "^4.3.0",
        "typeorm": "^0.2.28",
        "typescript": "^4.0.3",
        "uuid": "^8.3.2",
        "yup": "^0.29.3"
    }
}
